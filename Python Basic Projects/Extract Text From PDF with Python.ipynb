   "source": [
    "import PyPDF2\n",
    "pdf = open(\"Statistics Programming_Class Assessment.pdf\",'rb')\n",
    "reader = PyPDF2.PdfFileReader(pdf)\n",
    "page = reader.getPage(0)\n",
    "print(page.extractText())"
